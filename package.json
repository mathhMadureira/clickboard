{
  "name": "clickboard",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@10.18.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "pnpm --filter @clickboard/web dev",
    "dev:api": "pnpm --filter @clickboard/api start:dev",
    "dev": "concurrently -n WEB,API -c blue,magenta \"pnpm dev:web\" \"pnpm dev:api\""
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "dependencies": {
    "@nestjs/config": "^4.0.2"
  }
}
